    <!-- UTM capture: persist and attach to every event, then send initial $pageview -->
    <script>
        (function () {
            function parseUtmParams() {
                var url = new URL(window.location.href);
                var p = {};
                var keys = ['utm_source','utm_medium','utm_campaign','utm_term','utm_content','gclid','fbclid'];
                for (var i = 0; i < keys.length; i++) {
                    var v = url.searchParams.get(keys[i]);
                    if (v) p[keys[i]] = v;
                }
                return p;
            }

            function getInitialReferrerProps() {
                var ref = document.referrer || '';
                var domain = null;
                try { if (ref) domain = new URL(ref).hostname; } catch (e) {}
                return {
                    initial_referrer: ref || 'direct',
                    initial_referring_domain: domain || 'direct'
                };
            }

            try {
                var stored = null;
                try { stored = JSON.parse(localStorage.getItem('utm_params') || 'null'); } catch (e) {}
                var utm = stored || parseUtmParams();
                if (utm && Object.keys(utm).length > 0 && !stored) {
                    try { localStorage.setItem('utm_params', JSON.stringify(utm)); } catch (e) {}
                }

                var props = Object.assign({}, utm || {}, getInitialReferrerProps());
                if (Object.keys(props).length > 0) {
                    posthog.register(props);
                }
            } catch (e) {}

            // Now send the initial pageview with UTM props registered
            try { posthog.capture('$pageview'); } catch (e) {}
        })();
    </script>
    
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            function getNearestSectionId(element) {
                var node = element;
                while (node && node !== document.body) {
                    if (node.id) return node.id;
                    node = node.parentElement;
                }
                return null;
            }

            function getElementLabel(element) {
                var label = element.getAttribute('aria-label') || element.innerText || '';
                return label.trim().slice(0, 100);
            }

            // Track clicks on primary CTAs (buttons and links)
            document.body.addEventListener('click', function (event) {
                var target = event.target.closest('a.btn-primary, button.btn-primary');
                if (!target) return;

                var properties = {
                    text: getElementLabel(target),
                    href: target.getAttribute('href') || null,
                    section: getNearestSectionId(target),
                    path: window.location.pathname
                };

                try { posthog.capture('cta_clicked', properties); } catch (e) {}
            });

            // Track contact form submit success/error
            var contactForm = document.querySelector('section#contact form');
            if (contactForm) {
                // Invalid field error during submission attempt
                contactForm.addEventListener('invalid', function (ev) {
                    var field = ev.target;
                    var props = {
                        field: field.name || field.id || 'unknown',
                        reason: 'invalid',
                        section: 'contact',
                        path: window.location.pathname
                    };
                    try { posthog.capture('form_submit_error', props); } catch (e) {}
                }, true);

                // Successful submit (HTML5 validation passed). Formspree handles the POST + redirect
                contactForm.addEventListener('submit', function () {
                    try { posthog.capture('form_submit_success', { section: 'contact', path: window.location.pathname }); } catch (e) {}
                });
            }
        });
    </script>
    
    <!-- Scroll-depth and outbound link tracking -->
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Scroll depth tracking at 25/50/75/100%
            var thresholds = [25, 50, 75, 100];
            var fired = { 25: false, 50: false, 75: false, 100: false };
            function computeScrollPercent() {
                var doc = document.documentElement;
                var body = document.body;
                var scrollTop = (window.pageYOffset || doc.scrollTop) - (doc.clientTop || 0);
                var viewport = window.innerHeight || doc.clientHeight;
                var fullHeight = Math.max(
                    body.scrollHeight, doc.scrollHeight,
                    body.offsetHeight, doc.offsetHeight,
                    body.clientHeight, doc.clientHeight
                );
                var pct = Math.round(((scrollTop + viewport) / fullHeight) * 100);
                if (pct < 0) pct = 0;
                if (pct > 100) pct = 100;
                return pct;
            }
            function checkThresholds() {
                var pct = computeScrollPercent();
                for (var i = 0; i < thresholds.length; i++) {
                    var t = thresholds[i];
                    if (!fired[t] && pct >= t) {
                        fired[t] = true;
                        try { posthog.capture('scroll_depth', { percent: t, path: window.location.pathname }); } catch (e) {}
                    }
                }
            }
            var ticking = false;
            window.addEventListener('scroll', function () {
                if (!ticking) {
                    window.requestAnimationFrame(function () {
                        checkThresholds();
                        ticking = false;
                    });
                    ticking = true;
                }
            }, { passive: true });
            // Run once in case page is short
            checkThresholds();

            // Outbound link tracking (includes mailto/tel)
            document.body.addEventListener('click', function (event) {
                var link = event.target && event.target.closest ? event.target.closest('a[href]') : null;
                if (!link) return;
                var rawHref = link.getAttribute('href') || '';
                try {
                    var url = new URL(rawHref, window.location.href);
                    var isExternal = url.origin !== window.location.origin || url.protocol === 'mailto:' || url.protocol === 'tel:';
                    if (!isExternal) return;
                    var props = {
                        href: url.href,
                        text: (link.getAttribute('aria-label') || link.innerText || '').trim().slice(0, 100),
                        target: link.getAttribute('target') || '',
                        rel: link.getAttribute('rel') || '',
                        protocol: url.protocol.replace(':', ''),
                        host: url.host,
                        path: window.location.pathname
                    };
                    try { posthog.capture('outbound_click', props); } catch (e) {}
                } catch (err) {
                    // Fallback for malformed hrefs
                    try { posthog.capture('outbound_click', { href: rawHref, text: (link.innerText || '').trim().slice(0, 100), path: window.location.pathname }); } catch (e) {}
                }
            });
        });
    </script>
    
